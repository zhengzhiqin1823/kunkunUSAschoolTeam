<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace:命名空间,做sql隔离 -->

<mapper namespace="com.mapper.messageMapper">
    <select id="selectByKey" resultType="com.test.pojo.message">
        select * from messageID=#{messageID};
    </select>
    <select id="selectAll" resultType="com.test.pojo.message">
        select * from message;
    </select>
    <insert id="insert" parameterType="com.test.pojo.message">
        insert into message
        values (#{messageID},#{From},#{To},#{Title},#{firstFm},#{totalSize},#{isRead},#{time});
    </insert>
    <delete id="deleteByKey" parameterType="com.test.pojo.message">
        delete from message where messageID=#{messageID};
    </delete>
</mapper>