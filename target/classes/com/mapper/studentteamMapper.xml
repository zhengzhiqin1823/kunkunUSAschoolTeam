<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace:命名空间,做sql隔离 -->

<mapper namespace="com.mapper.studentteamMapper">
    <insert id="insert" parameterType="com.test.pojo.studentteam">
        insert  into student_team
        values(#{sid},#{teamID});
    </insert>
    <select id="selectByKey" resultType="com.test.pojo.studentteam">
        select * from student_team where  sid=#{sid} and teamID=#{teamID};
    </select>
    <select id="selectAll" resultType="com.test.pojo.studentteam">
        select * from student_team;
    </select>
    <delete id="deleteByKey" parameterType="com.test.pojo.studentteam">
        delete from student_team where sid=#{sid} and teamID=#{teamID};
    </delete>
</mapper>